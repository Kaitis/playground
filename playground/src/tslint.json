{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "selector-pseudo-element-no-unknown": [true, {
          "ignorePseudoElements": ["ng-deep"]
        }]
    }
}
